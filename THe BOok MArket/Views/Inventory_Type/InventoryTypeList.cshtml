@model IEnumerable<THe_BOok_MArket.Models.Inventory_Type>

@{
    
    Layout = "~/Views/Shared/Layout12.cshtml";
}


<h2>Inventory Type Information</h2>

@Html.ActionLink("Export to Excel", "Reports", new { ReportType = "Excel" }) |
@Html.ActionLink("Export to PDF", "Reports", new { ReportType = "PDF" }) |
@Html.ActionLink("Export to Word", "Reports", new { ReportType = "Word" }) |
@Html.ActionLink("Export to Image", "Reports", new { ReportType = "Image" }) |


<br /><br />

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.InventoryType_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InventoryType_Description)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InventoryType_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InventoryType_Description)
            </td>

        </tr>
    }

</table>
